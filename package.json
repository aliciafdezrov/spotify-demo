    "sass": "1.37.0",
    "sass-loader": "12.1.0",
    "style-loader": "3.2.1",
    "ts-jest": "27.0.4",
    "typescript": "4.3.5",
    "webpack": "5.48.0",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-merge": "5.8.0"
  }
