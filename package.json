{
  "name": "empathy-takehome",
  "version": "1.0.0",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start:dev": "webpack serve --config ./config/webpack/dev.webpack.js",
    "start:prod": "webpack serve --config ./config/webpack/pro.webpack.js",
    "build:dev": "rimraf dist && npm run type-check && webpack --config ./config/webpack/dev.webpack.js",
    "build:prod": "rimraf dist && npm run type-check && webpack --config ./config/webpack/pro.webpack.js",
    "build:perf": "rimraf dist && npm run type-check && webpack --config ./config/webpack/perf.webpack.js",
    "test": "rimraf coverage && jest -c ./config/test/jest.js --verbose --coverage",
    "test:watch": "npm run test -- --watchAll -i",
    "cypress": "run-p --race start & cypress open"
  },
  "license": "MIT",
  "author": "Alicia Fern√°ndez Rovira",
  "dependencies": {
    "lodash.debounce": "4.0.8",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "^5.2.0",
    "regenerator-runtime": "0.13.9"
  },
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.9",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.14.5",
    "@testing-library/dom": "8.1.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.1",
    "@testing-library/user-event": "13.2.1",
    "@types/jest": "26.0.24",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.0.1",
    "cypress": "8.2.0",
    "dotenv-webpack": "7.0.3",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "jest": "27.0.6",
    "jest-transform-stub": "2.0.0",
    "mini-css-extract-plugin": "2.1.0",
    "npm-run-all": "4.1.5",
